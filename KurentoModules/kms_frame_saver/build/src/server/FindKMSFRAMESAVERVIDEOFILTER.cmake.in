if (TARGET kmsframesavervideofilterimpl)
  set (KMSFRAMESAVERVIDEOFILTER_LIBRARY kmsframesavervideofilterimpl)
else ()
  find_library (KMSFRAMESAVERVIDEOFILTER_LIBRARY
    NAMES
      kmsframesavervideofilterimpl
    PATH_SUFFIXES
      ${KMSFRAMESAVERVIDEOFILTER_BINARY_DIR_PREFIX}/src/server
  )
endif()

set (REQUIRED_LIBS "@REQUIRED_LIBS@")
foreach (LIB ${REQUIRED_LIBS})
  string(FIND ${LIB} " " POS)

  if (${POS} GREATER 0)
    string(REPLACE " " ";" REQUIRED_LIB_LIST ${LIB})
    include (CMakeParseArguments)
    cmake_parse_arguments("PARAM" "" "LIBNAME" "" ${REQUIRED_LIB_LIST})

    if (DEFINED PARAM_LIBNAME)
      generic_find (${REQUIRED_LIB_LIST} REQUIRED)
      set (LIB_NAME ${PARAM_LIBNAME})
    else()
      string (SUBSTRING ${LIB} 0 ${POS} LIB_NAME)
      string (SUBSTRING ${LIB} ${POS} -1 LIB_VERSION)
      string (STRIP ${LIB_NAME} LIB_NAME)
      string (STRIP ${LIB_VERSION} LIB_VERSION)
      generic_find (LIBNAME ${LIB_NAME} REQUIRED VERSION "${LIB_VERSION}")
    endif()
  else ()
    string (STRIP ${LIB} LIB_NAME)
    generic_find (LIBNAME ${LIB_NAME} REQUIRED)
  endif ()
  list (APPEND REQUIRED_LIBRARIES ${${LIB_NAME}_LIBRARIES})
  list (APPEND KMSFRAMESAVERVIDEOFILTER_INCLUDE_DIRS ${${LIB_NAME}_INCLUDE_DIRS})

endforeach()

set(KMSFRAMESAVERVIDEOFILTER_INCLUDE_DIRS
  ${KMSFRAMESAVERVIDEOFILTER_INCLUDE_DIRS}
  CACHE INTERNAL "Include directories for KMSFRAMESAVERVIDEOFILTER library" FORCE
)

set (KMSFRAMESAVERVIDEOFILTER_LIBRARIES
  ${KMSFRAMESAVERVIDEOFILTER_LIBRARY}
  ${KMSCORE_LIBRARIES}
  ${KMSELEMENTS_LIBRARIES}
  ${KMSFILTERS_LIBRARIES}
  ${REQUIRED_LIBRARIES}
  CACHE INTERNAL "Libraries for KMSFRAMESAVERVIDEOFILTER" FORCE
)

include (FindPackageHandleStandardArgs)

find_package_handle_standard_args(KMSFRAMESAVERVIDEOFILTER
  FOUND_VAR
    KMSFRAMESAVERVIDEOFILTER_FOUND
  REQUIRED_VARS
    ${REQUIRED_VARS}
  VERSION_VAR
    KMSFRAMESAVERVIDEOFILTER_VERSION
)

mark_as_advanced(
  KMSFRAMESAVERVIDEOFILTER_FOUND
  KMSFRAMESAVERVIDEOFILTER_VERSION
  KMSFRAMESAVERVIDEOFILTER_INTERFACE_INCLUDE_DIR
  KMSFRAMESAVERVIDEOFILTER_IMPLEMENTATION_INTERNAL_INCLUDE_DIR
  KMSFRAMESAVERVIDEOFILTER_IMPLEMENTATION_GENERATED_INCLUDE_DIR
  KMSFRAMESAVERVIDEOFILTER_IMPLEMENTATION_EXTRA_INCLUDE_DIR
  KMSFRAMESAVERVIDEOFILTER_INTERFACE_EXTRA_INCLUDE_DIR
  KMSFRAMESAVERVIDEOFILTER_INCLUDE_DIRS
  KMSFRAMESAVERVIDEOFILTER_LIBRARY
  KMSFRAMESAVERVIDEOFILTER_LIBRARIES
)
