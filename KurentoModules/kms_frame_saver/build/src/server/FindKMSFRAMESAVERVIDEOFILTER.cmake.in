message (STATUS "Looking for KMSFRAMESAVERVIDEOFILTER: @PROJECT_VERSION@")

include (GenericFind)

generic_find (
  REQUIRED
  LIBNAME KMSCORE
  VERSION ^6.0.0
)

generic_find (
  REQUIRED
  LIBNAME KMSELEMENTS
  VERSION ^6.0.0
)

