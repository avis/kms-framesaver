    set (KMSFRAMESAVERVIDEOFILTER_INTERFACE_INCLUDE_DIR "@_INTERFACE_HEADERS_DIR@")
  else ()
    find_path(KMSFRAMESAVERVIDEOFILTER_INTERFACE_INCLUDE_DIR
      NAMES
        @_INTERFACE_GENERATED_HEADERS@
        @_INTERFACE_INTERNAL_GENERATED_HEADERS@
      PATH_SUFFIXES
        @_INTERFACE_HEADERS_DIR@
        kurento/modules/framesavervideofilter
    )
  endif ()

  list (APPEND KMSFRAMESAVERVIDEOFILTER_INCLUDE_DIRS ${KMSFRAMESAVERVIDEOFILTER_INTERFACE_INCLUDE_DIR})
  list (APPEND REQUIRED_VARS KMSFRAMESAVERVIDEOFILTER_INTERFACE_INCLUDE_DIR)
endif ()

